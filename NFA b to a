#include<stdio.h>
#include<string.h>
main()
{
	char s[40],B='b';
	int n,c=0;
	printf("enter the stirng:");
	scanf("%s",s);
	n=strlen(s);
	for (int i=0;i<n;i++)
	{
		if(s[i]=='b'|| s[i]=='a')
		{
			c=0;
		}
		else
		{
			c=1;
			printf("invalid");
			break;
		}
	}
	if(c==0)
	{
	if(s[0]=='b' && s[n-1]=='a')
	{
		printf("nfa");
	}
	else{
		printf("not nfa");
	}
	}
}
